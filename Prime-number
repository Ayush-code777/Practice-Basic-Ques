#include <stdio.h>
#include <stdlib.h>
#include <math.h>

void prime(int x){
    int count=0;
    for(int i=1;i*i<=x;i++){
        if(x%i==0){
            count++;
        }
        else{
            continue;
        }

    }
    if(count==1){
        printf("The number is a prime number\n");

    }else{
        printf("The number is not a prime number\n");
    }

}


int main(int argc, char const *argv[])
{

    int num=3457;
    prime(num);
    /* code */
    return 0;
}

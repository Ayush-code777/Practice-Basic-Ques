#include <stdio.h>
#include <stdlib.h>


void mul(int x){
    for(int i=1;i<=10;i++){
        int b=x*i;
        printf("%d x %d   : %d\n",x,i,b);
    }

}

int main(int argc, char const *argv[])
{
    int num;
    printf("Enter the number\n");
    scanf("%d",&num);
    mul(num);
    /* code */
    return 0;
}

#include <stdio.h>
#include <string.h>

//#include <cstring>
#include <stdlib.h>



int main(int argc, char const *argv[])
{
    char *s="lluurrdd";
    int count=0;
    int i=0;
    while(s[i]!='\0'){
        if (s[i]=='r' || s[i]=='u'){
            count+=1;
            i++;
        }
        else if(s[i]=='l' || s[i]=='d')
        {
            count-=1;
            i++;
        }


    }
    if(count==0){
        printf("TRUE\n");
    }
    else{
        printf("FALSE\n");
    }
    /* code */
    return 0;
}
